        "class Human:\n",
        "  def __init__(self,name,age):\n",
        "      self.name = name\n",
        "      self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "      return f\"名前は、{self.name}で,年齢は{self.age}歳です。\"\n",
        "\n",
        "personal = Human(\"侍太郎\",36)\n",
        "\n",
        "print(personal.printinfo())\n"
